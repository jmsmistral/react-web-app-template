{
  "name": "react-web-app-template",
  "version": "0.1.0",
  "description": "A template for kick-starting react web applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmsmistral/react-web-app-template"
  },
  "main": "app.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./src/**/*.js ./test/**/*.js",
    "build:client:dev": "./node_modules/.bin/webpack",
    "build:client:prod": "./node_modules/.bin/webpack --env production",
    "build-babel:client": "./node_modules/.bin/babel ./src/client/app.js --out-dir ./public/dist/",
    "test:client": "./node_modules/.bin/jest ./tests/client/ --collectCoverageFrom=**/tests/client/**/*.test.js",
    "test:server": "./node_modules/.bin/mocha --require @babel/register ./tests/server/**/*.test.js",
    "dev-server:client": "./node_modules/.bin/webpack serve",
    "dev-server:client:docker": "./node_modules/.bin/webpack serve --host 0.0.0.0",
    "start:client": "DEBUG=app* nodemon ./src/client/server.js",
    "build:blog": "node ./src/client/utils/mdToHtmlConverter.js",
    "predeploy": "npm run build:blog && npm run build:client:prod",
    "deploy": "./node_modules/gh-pages/bin/gh-pages.js -d ./public"
  },
  "keywords": [
    "react"
  ],
  "author": "Jonathan Sacramento",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "bootstrap": "4.5.0",
    "chalk": "^4.1.0",
    "css-loader": "^5.0.1",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "gh-pages": "^3.2.3",
    "highlight.js": "^10.4.0",
    "jquery": "^3.5.1",
    "marked": "^2.0.0",
    "mini-css-extract-plugin": "^1.3.1",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.6",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-html": "6.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "jest": "24.8.0",
    "mocha": "6.1.4",
    "nock": "10.0.6",
    "sinon": "7.3.2",
    "webpack-dev-server": "^3.11.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 3000,
      "VIEWS_DIR": "views"
    }
  }
}
